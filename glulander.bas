sub welcome_screen()
    cls
    border 0
    paper 0
    ink 6

    for i = 0 to 23
        for j = 0 to 31
            print at i, j; chr$(146)
        next j
    next i

    print at 2, 6; ink 7; "                    ";
    print at 3, 6; ink 4; "  GLULANDER V1.0.0  ";
    print at 4, 6; ink 7; "                    ";
    print at 5, 6; ink 7; "                    ";
    print at 6, 6; ink 5; " "; chr$(127); " 2024 VASCO COSTA ";
    print at 7, 6; ink 7; "                    ";
    print at 9, 6; ink 7; "                    ";
    print at 10, 6; ink 4; "        KEYS        ";
    print at 11, 6; ink 7; "                    ";
    print at 12, 6; ink 7; "                    ";
    print at 13, 6; ink 7; "   SPACE - THRUST   ";
    print at 14, 6; ink 7; "   O - MOVE LEFT    ";
    print at 15, 6; ink 7; "   P - MOVE RIGHT   ";
    print at 16, 6; ink 7; "                    ";
    print at 18, 6; ink 7; "                    ";
    print at 19, 6; ink 7; "                    ";
    print at 20, 6; ink 7; "                    ";

    for i = 7 to 23
        print at 19, i; ink 7; chr$(144);
        beep 0.2, int(rnd() * 10)
        print at 19, i; ink 7; chr$(32);
    next i

    print at 19, 6; ink 7; "  PRESS "; ink 4; "S"; ink 7; " TO START  ";
    do loop while inkey$ <> "s"

    cls

    print "The year is 1569 and Portugal"
    print "developed a crazy space program"
    print "for the time, using a cannon to"
    print "toss a lander into lunar orbit."
    print ""
    print "The lander is called Lua and is"
    print "made of, well, wood. What else?"
    print "The vertical thrust system is a"
    print "homemade jet of sewage water,"
    print "however, the lateral guidance"
    print "system is state of the art and"
    print "stabilised to avoid drifting."
    print ""
    print "Try to land on the green pads,"
    print "by keeping the vertical speed"
    print "denoted by VY below 5.8 m/s."
    print at 19, 6; ink 7; "  PRESS "; ink 4; "S"; ink 7; " TO START  ";
end sub

sub main_loop()
    do
        init_vars()
        if crashed = 0 then
            load_level()
        end if
        render_level()
        update()
        do loop while inkey$ <> "n"
    loop while level <= MAX_LEVEL and lives > 0

    game_over()
end sub

sub init_vars()
    let x = 16
    let y = 0
    let vy = 0
    let gravity = 0.08
    let thrust = -0.2
    let fuel = 50
    let lc$ = chr$(144)
    let lcc$ = chr$(145)
end sub

sub render_level()
    border 1
    paper 0
    cls

    for i = 0 to 21
        for j = 0 to 31
            c = current_level(i, j)
            if c = 88 then
                ink 4
                paper 4
                bright 1
            else if c = 89
                ink 6
                paper 6
                bright 0
            else
                ink 6
                paper 0
                bright 0
            end if
            print at i, j; chr$(c)
        next j
    next i

    print at 0, 0; paper 7; ink 1; right("00" + str$(level), 2)
end sub

sub render_flag()
    print at 16, 21; ink 4; paper 4; "  "; ink 2; paper 2; "   ";
    print at 17, 21; ink 4; paper 4; "  "; ink 2; paper 2; "   ";
    print at 18, 21; ink 7; paper 7; " ";
end sub

sub load_level()
    for i = 0 to 21
        for j = 0 to 31
            read a
            current_level(i, j) = a
        next j
    next i
end sub

function level_score(landing_points as uinteger, crash_points as uinteger) as uinteger
    return landing_points + crash_points
end function

sub update()
    do
        rem Clear
        print at y, x; " "

        rem Apply gravity
        let vy = vy + gravity

        rem Read player input
        let a$ = inkey$
        if a$ = " " > 0 and fuel > 0 then
            let vy = vy + thrust
            let fuel = fuel - 1
            let total_fuel = total_fuel + 1
        end if
        let a$ = inkey$
        if a$ = "o" > 0 and x >= 1 then
            if screen$(int(y), x - 1) <> "Y" then
                let x = x - 1
            else
                crash()
                return
            end if
        end if
        let a$ = inkey$
        if a$ = "p" > 0 and x <= 30 then
            if screen$(int(y), x + 1) <> "Y" then
                let x = x + 1
            else
                crash()
                return
            end if
        end if

        rem Update position
        if y + vy >= 0 then
            let y = y + vy
        end if
        print at int(y), x; ink 7; lc$

        rem Check if landed or crashed
        if screen$(int(y) + 1, x) = "X" or screen$(int(y) + 1, x) = "Y" then
            if vy < 1.2 and screen$(int(y) + 1, x) = "X" then
                land()
                return
            else
                crash()
                return
            end if
            do loop while inkey$ <> "n"
        end if
        if screen$(int(y) - 1, x) = "Y" and vy < 0 then
            crash()
            return
        end if

        show_stats()

        rem Delay for smoother movement
        for i = 0 to 2000: next i
    loop
end sub

sub show_stats()
    print at 22, 0; ink 5; bright 1; "FUEL (L): "; ink 3; right("00" + str$(fuel), 2);
    print at 23, 0; ink 5; bright 1; "VY (M/S):            ";
    print at 23, 0; ink 5; bright 1; "VY (M/S): "; ink 3; vy * 5;
    rem print at 22, 21; ink 5; bright 1; "LIVES: "; ink 7; right("0000" + str$(lives), 4);
    print at 22, 21; ink 5; bright 1; "LIVES: ";
    ink 7
    for l = 1 to lives
        print at 22, 27 + l; lc$;
    next l 
    print at 23, 21; ink 5; bright 1; "SCORE: "; ink 7; right("0000" + str$(score), 4);
end sub

sub crash()
    print at y, x; ink 2; lcc$;
    print at 1, 2; ink 2; "CRASHED! PRESS N TO CONTINUE.";
    border 2

    beep 0.15, -5
    beep 0.15, 5
    beep 0.15, -10
    beep 0.2, -5

    let lives = lives - 1
    let crashed = 1
end sub

sub land()
    print at 1, 2; ink 4; "LANDED! PRESS N TO CONTINUE.";
    border 4

    if level = MAX_LEVEL
        render_flag()
    end if

    beep 0.15, 5
    beep 0.15, 10
    beep 0.15, 7
    beep 0.2, 12

    let score = score + level_score(50, 0)
    let crashed = 0
    let level = level + 1
end sub

sub game_over()
    border 0
    paper 0
    cls
    if level = 11 then
        print at 8, 10; ink 0; paper 4; "  YOU WON!  ";
    else
        print at 8, 10; ink 7; paper 2; " GAME OVER! ";
    end if
    bright 1

    if score = MAX_SCORE then
        if total_fuel <= 250 then
            let bonus = MAX_SCORE
        else if total_fuel > 250 and total_fuel <= 260 then
            let bonus = MAX_SCORE / 2
        else if total_fuel > 260 and total_fuel <= 270 then
            bonus = MAX_SCORE / 4
        end if
    end if

    print at 11, 10; ink 7; "LEVEL:  "; right("0000" + str$(level - 1), 4);
    print at 12, 10; ink 7; "TFUEL:  "; right("0000" + str$(total_fuel), 4);
    print at 13, 10; ink 7; "SCORE:  "; right("0000" + str$(score), 4);
    print at 14, 10; ink 7; "BONUS:  "; right("0000" + str$(bonus), 4);
    print at 16, 10; ink 1; paper 7; "SCORE:  "; right("0000" + str$(score + bonus), 4);
    
    if level = 11 then
        beep 0.3, 5
        beep 0.3, 7
        beep 0.3, 9
        beep 0.3, 12
        beep 0.2, 14
        beep 0.2, 16
        beep 0.2, 18
        beep 0.5, 20
    else
        beep 0.3, 10
        beep 0.25, 8
        beep 0.25, 6
        beep 0.25, 5
        beep 0.4, 4
        beep 0.2, -3
        beep 0.2, -5
        beep 0.5, -8
    end if
    
    bright 0
    paper 0
    print at 19, 6; ink 7; "  PRESS "; ink 4; "S"; ink 7; " TO START  ";
end sub

